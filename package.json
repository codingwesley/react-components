  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "cy:open": "cypress open-ct",
    "cy:run": "cypress run-ct"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/node-logger": "^6.4.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.7",
    "@storybook/testing-react": "^1.0.0",
    "cypress": "^9.1.1",
    "ts-jest": "^27.1.2",
    "webpack-dev-server": "^4.6.0"
  }
